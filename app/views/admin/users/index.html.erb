<h1>All Users</h1>
<a href="/admin/users/new"><h2>Add User</h2></a>
    <hr>
    <% @user.reverse.each do |user| %>
      <a href="/admin/users/<%= user.id %>"><h4><%= user.firstname %>, <%= user.lastname %></h4></a>
      <h5><%= user.email %></h5>
      <h4>ADMIN: <%= user.admin %></h4>
      <p>Join Date: <%= user.created_at %></p>
      <hr>
    <% end %>
